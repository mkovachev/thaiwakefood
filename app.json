{
  "expo": {
    "name": "thaiwakefood",
    "slug": "thaiwakefood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "thaiwakefood",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#fff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/logo.png",
      "build": {
        "babel": {}
      }
    }
  }
}